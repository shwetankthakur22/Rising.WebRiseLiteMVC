
@{
    ViewBag.Title = "BrokrageDetails";
    Layout = "~/Views/Shared/_LayoutClient.cshtml";
}




<div class="page-wrapper">
    <!-- Page Content-->
    <div class="page-content">
        <!-- Page-Title -->
        <div class="row">
            <div class="col-sm-12">
                <div class="page-title-box">
                    <div class="row">


                    </div>
                    <!--end row-->
                </div>
                <!--end page-title-box-->
            </div>
            <!--end col-->
        </div>

        <div class="row">
            <div class="col-lg-12" id="Ledger" style="margin: auto;">
                <div class="card" style="margin-top: 20px;">
                    <div class="card-header" style="background-color:#6496d8;">
                        <h4 class="card-title" style="color:white">Brokrage Details</h4>
                    </div>
                    <!--end card-header-->
                    <div class="card-body">
                       
                        <!--end row-->


                        <div class="row">
                            <div class="col-lg-12">
                                <div style="float:right;padding:2px 2px 2px 2px">

                                    <a href="javascript:void(0);" class="btn btn-dark" onclick="history.go(-1);"><i class="fa fa-arrow-left"></i></a>

                                </div>
                            </div>
                            <div class="col-lg-12" style="margin: auto;">
                                <div class="card" id="ExportedGrid">
                                    <div class="card-body" style="padding: 0;">
                                        <div class="table-responsive browser_users">
                                            <div class="table-responsive table-box">
                                                
                                                    <table class="table table-striped table-bordered" style="width:100%;">
                                                        <thead>
                                                            <tr>                                                               
                                                                <th style="border: 1px solid  #808080;text-align:center; background-color:#6496d8;font-size: 12px;font-weight: bold;color: white;">TRADE TYPE</th>
                                                                <th style="border: 1px solid  #808080;text-align:center; background-color:#6496d8;font-size: 12px;font-weight: bold;color: white;">BROK TYPE</th>
                                                                <th style="border: 1px solid  #808080;text-align:center; background-color:#6496d8;font-size: 12px;font-weight: bold;color: white;">FIXED BROK</th>
                                                                <th style="border: 1px solid  #808080;text-align:center; background-color:#6496d8;font-size: 12px;font-weight: bold;color: white;">FIXED MIN</th>
                                                                <th style="border: 1px solid  #808080;text-align:center; background-color:#6496d8;font-size: 12px;font-weight: bold;color: white;">Option</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            @{
                                                                int rowCount = Model.ds.Tables[0].Rows.Count;
                                                                for (int i = 0; i < rowCount; i++)
                                                                {
                                                                    <tr>                                                                        
                                                                        <td style="text-align: center;font-size: 12px;border: 1px solid  #d3d3d3;">@Model.ds.Tables[0].Rows[i][1]</td>
                                                                        <td style="text-align: center;font-size: 12px;border: 1px solid  #d3d3d3;">@Model.ds.Tables[0].Rows[i][2]</td>
                                                                        <td style="text-align: center;font-size: 12px;border: 1px solid  #d3d3d3;">@Model.ds.Tables[0].Rows[i][3]</td>
                                                                        <td style="text-align: center;font-size: 12px;border: 1px solid  #d3d3d3;">@Model.ds.Tables[0].Rows[i][4]</td>
                                                                        <td style="text-align: center;font-size: 12px;border: 1px solid  #d3d3d3;">@Model.ds.Tables[0].Rows[i][5]</td>
                                                                    </tr>
                                                                }


                                                            }
                                                            
                                                        </tbody>
                                                    </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

