@model Rising.WebRise.Models.Reports.ContractNoteCustodian

@{
    ViewBag.Title = "ContractNoteCustodian";
    Layout = "~/Views/Shared/_LayoutClient.cshtml";}

<div class="row">
    <div class="col-xl-12 col-lg-12">
        <div class="card">
            <div class="card-header d-flex justify-content-between">
                <div class="header-title">
                    <h4 class="card-title">Contract Note Custodian</h4>
                </div>
            </div>
            <div class="card-body">
                <div class="new-user-info">
                    @using (Html.BeginForm("ContractNoteCustodian", "Master", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "", role = "form" }))
                    {
                        @Html.AntiForgeryToken()
                        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                        <div class="row">
                            <div class="col-md-12">
                                <div class="row">
                                    <div class="col-md-2">
                                        @Html.LabelFor(m => m.ClientCode, new { @class = "form-label" })
                                    </div>
                                    <div class="col-md-2">
                                        @Html.TextBoxFor(m => m.ClientCode, new { @class = "form-control" })
                                    </div>
                                    <div class="col-md-1">
                                        <button type="button" class="btn btn-sm btn-primary" data-toggle="modal" data-target="#CodeFrom"><i class="ri-search-fill pr-0"></i></button>
                                    </div>
                                    <div class="col-md-5">
                                        @Html.TextBoxFor(m => m.ClientCode, new { @class = "form-control" })
                                    </div>
                                    </div>
                                <div class="row">
                                    <div class="col-md-2">
                                        @Html.LabelFor(m => m.Exchange, new { @class = "form-label" })
                                    </div>
                                    <div class="col-md-2">
                                        @Html.DropDownListFor(m => m.Exchange, new SelectList(new[] { "NSE", "INX" }), new { @class = "form-control" })
                                    </div>
                                    <div class="col-md-2">
                                        @Html.LabelFor(m => m.TradeDate, new { @class = "form-label" })
                                    </div>
                                    <div class="col-md-2">
                                        @Html.TextBoxFor(m => m.TradeDate, new { @class = "form-control" })
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group col-12 ">
                                <button type="submit" class="btn btn-primary">Cancel</button>
                                <button type="submit" class="btn btn-primary pull-right">Save</button>
                            </div>
                        </div>
                    }
                </div>
                <!-- container -->
                <!--end footer-->
                <div class="row mt-100">
                    <div class="col-lg-12 col-md-12 col-sm-12" style="margin: auto;">
                        <div class="card" id="ExportedGrid">
                            <div class="box">
                                <div>
                                    <table class="table-bordered table-box table">
                                        <thead>
                                            <tr>
                                                <th class="table-header">Code</th>
                                                <th class="table-header">Name</th>
                                                <th class="table-header">Symbol</th>
                                                <th class="table-header">Expiry Date</th>
                                                <th class="table-header">Strike Price</th>
                                                <th class="table-header">Option Type</th>
                                                <th class="table-header">Qty.</th>
                                                <th class="table-header">Mkt. Rate</th>
                                                <th class="table-header">Value</th>
                                                <th class="table-header">Brok</th>
                                                <th class="table-header">Trxn. Chrg</th>
                                                <th class="table-header">SEBI</th>
                                                <th class="table-header">Stamp</th>
                                            </tr>
                                        </thead>
                                        <tbody></tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                        <!--end card-body-->
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
