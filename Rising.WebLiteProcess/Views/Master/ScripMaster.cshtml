}


@section Scripts {
    <script>
            debugger
            $(".selectsttl").click(function () {
                $("#ScripCode").val("").html("");
                $("#ISINCode").val("").html("");
                $("#ScripName").val("").html("");
                $("#Remark").val("").html("");
                $("#LotSize").val("").html("");
                $("#Variance").val("").html("");
                $("#CustomVariance").val("").html("");
                $("#MarginSale").val("").html("");
                $("#MarginBuy").val("").html("");
                $("#GroupCD").val("").html("");
                $("#ClosingStockDemat").val("").html("");
                $("#DematScrip").val("").html("");             
                $("#TrnxScripType").val("").html("");
                $("#Pledgeable").val("").html("");
                $("#STTCalculate").val("").html("");
                $("#DPPreference").val("").html("");
                $("#MTFMovement").val("").html("");
                $("#BondScrip").val("").html("");
                $("#FundFlag").val("").html("");
                $("#IgnoreTrnxTax").val("").html("");
                $("#EquityOrientedFund").val("").html("");
                $("#DelTradeFO").val("").html("");
                $("#RowId").val("").html("");
             
                var id = $(this).data("model-id");
                debugger
                $.ajax({
                    url: '/Master/ScripMasterSelect/',
                    type: "GET",
                    dataType: "JSON",
                    data: { rid: id },
                    success: function (Scriptype) {
                        debugger                   
                        $("#ScripCode").val(Scriptype.ScripCode).html(Scriptype.ScripCode);
                        $("#ISINCode").val(Scriptype.ISINCode).html(Scriptype.ISINCode);
                        $("#ScripName").val(Scriptype.ScripName).html(Scriptype.ScripName);
                        $("#Remark").val(Scriptype.Remark).html(Scriptype.Remark);
                        $("#LotSize").val(Scriptype.LotSize).html(Scriptype.LotSize);
                        $("#Variance").val(Scriptype.Variance).html(Scriptype.Variance);
                        $("#CustomVariance").val(Scriptype.CustomVariance).html(Scriptype.CustomVariance);
                        $("#MarginSale").val(Scriptype.MarginSale).html(Scriptype.MarginSale);
                        $("#MarginBuy").val(Scriptype.MarginBuy).html(Scriptype.MarginBuy);
                        $("#GroupCD").val(Scriptype.GroupCD).html(Scriptype.GroupCD);
                        $("#ClosingStockDemat").val(Scriptype.MarginBuy).html(Scriptype.MarginBuy);           
                        $("#TrnxScripType").val(Scriptype.TrnxScripType).html(Scriptype.TrnxScripType);
                        $("#RowId").val(Scriptype.RowId).html(Scriptype.RowId);
                        $("#Pledgeable").append($("<option>", { value: Scriptype.Pledgeable, text: Scriptype.Pledgeable }))
                        if (Scriptype.Pledgeable == "Y") $("#Pledgeable").append($("<option>", { value: 'N', text: 'N' }))
                        if (Scriptype.Pledgeable == "N") $("#Pledgeable").append($("<option>", { value: 'Y', text: 'Y' }))
                        $("#STTCalculate").append($("<option>", { value: Scriptype.STTCalculate, text: Scriptype.STTCalculate }))
                        if (Scriptype.STTCalculate == "Y") $("#STTCalculate").append($("<option>", { value: 'N', text: 'N' }))
                        if (Scriptype.STTCalculate == "N") $("#STTCalculate").append($("<option>", { value: 'Y', text: 'Y' }))
                        $("#DPPreference").append($("<option>", { value: Scriptype.DPPreference, text: Scriptype.DPPreference }))
                        if (Scriptype.DPPreference == "Y") $("#DPPreference").append($("<option>", { value: 'N', text: 'N' }))
                        if (Scriptype.DPPreference == "N") $("#DPPreference").append($("<option>", { value: 'Y', text: 'Y' }))
                        $("#MTFMovement").append($("<option>", { value: Scriptype.MTFMovement, text: Scriptype.MTFMovement }))
                        if (Scriptype.MTFMovement == "Y") $("#MTFMovement").append($("<option>", { value: 'N', text: 'N' }))
                        if (Scriptype.MTFMovement == "N") $("#MTFMovement").append($("<option>", { value: 'Y', text: 'Y' }))
                        $("#BondScrip").append($("<option>", { value: Scriptype.BondScrip, text: Scriptype.BondScrip }))
                        if (Scriptype.BondScrip == "Y") $("#BondScrip").append($("<option>", { value: 'N', text: 'N' }))
                        if (Scriptype.BondScrip == "N") $("#BondScrip").append($("<option>", { value: 'Y', text: 'Y' }))
                        $("#FundFlag").append($("<option>", { value: Scriptype.FundFlag, text: Scriptype.FundFlag }))
                        if (Scriptype.FundFlag == "Y") $("#FundFlag").append($("<option>", { value: 'N', text: 'N' }))
                        if (Scriptype.FundFlag == "N") $("#FundFlag").append($("<option>", { value: 'Y', text: 'Y' }))
