                        $("#DematScrip").append($("<option>", { value: Scriptype.DematScrip, text: Scriptype.DematScrip }))
                        if (Scriptype.DematScrip == "Y") $("#DematScrip").append($("<option>", { value: 'N', text: 'N' }))
                        if (Scriptype.DematScrip == "N") $("#DematScrip").append($("<option>", { value: 'Y', text: 'Y' }))
                        $("#DelTradeFO").append($("<option>", { value: Scriptype.DelTradeFO, text: Scriptype.DelTradeFO }))
                        if (Scriptype.DelTradeFO == "Y") $("#DelTradeFO").append($("<option>", { value: 'N', text: 'N' }))
                        if (Scriptype.DelTradeFO == "N") $("#DelTradeFO").append($("<option>", { value: 'Y', text: 'Y' }))
                        $('#exampleModalLarge1').modal('hide');

                    }
                });

            });
