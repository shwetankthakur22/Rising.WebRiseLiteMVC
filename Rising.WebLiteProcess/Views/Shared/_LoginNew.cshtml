    <link href="~/Content/assets/css/owl.carousel.min.css" rel="stylesheet" type="text/css">
    <link href="~/Content/assets/css/owl.theme.default.min.css" rel="stylesheet" type="text/css">

    <!--=========================*
            Font Awesome
    *===========================-->
    <link rel="stylesheet" href="~/Content/assets/css/font-awesome.min.css">

    <!--=========================*
             Themify Icons
    *===========================-->
    <link rel="stylesheet" href="~/Content/assets/css/themify-icons.css">

    <!--=========================*
               Ionicons
    *===========================-->
    <link href="~/Content/assets/css/ionicons.min.css" rel="stylesheet" />

    <!--=========================*
              EtLine Icons
    *===========================-->
    <link href="~/Content/assets/css/et-line.css" rel="stylesheet" />

    <!--=========================*
              Feather Icons
    *===========================-->
    <link href="~/Content/assets/css/feather.css" rel="stylesheet" />

    <!--=========================*
              Modernizer
    *===========================-->
    <script src="~/Content/assets/js/modernizr-2.8.3.min.js"></script>

    <!--=========================*
               Metis Menu
    *===========================-->
    <link rel="stylesheet" href="~/Content/assets/css/metisMenu.css">

    <!--=========================*
               Slick Menu
    *===========================-->
    <link rel="stylesheet" href="~/Content/assets/css/slicknav.min.css">

    <!--=========================*
            Google Fonts
    *===========================-->
    <!-- Overpass USE: font-family: 'Overpass', sans-serif;-->
    <link href="https://fonts.googleapis.com/css?family=Overpass:100,200,300,400,600,700,800,900&amp;display=swap" rel="stylesheet">

    <!--=========================*
                Scripts
    *===========================-->
    <!-- Jquery Js -->
    <script src="~/Content/assets/js/jquery.min.js"></script>
    <!-- bootstrap 4 js -->
    <script src="~/Content/assets/js/popper.min.js"></script>
    <script src="~/Content/assets/js/bootstrap.min.js"></script>
    <!-- Owl Carousel Js -->
    <script src="~/Content/assets/js/owl.carousel.min.js"></script>
    <!-- Metis Menu Js -->
    <script src="~/Content/assets/js/metisMenu.min.js"></script>
    <!-- SlimScroll Js -->
    <script src="~/Content/assets/js/jquery.slimscroll.min.js"></script>
    <!-- Slick Nav -->
    <script src="~/Content/assets/js/jquery.slicknav.min.js"></script>
    <!-- Fancy Box Js -->
    <script src="~/Content/assets/js/jquery.fancybox.pack.js"></script>
    <!-- Main Js -->
    <script src="~/Content/assets/js/main.js"></script>
    
    <script type="text/javascript">
        function preventBack() { window.history.forward(); }
        setTimeout("preventBack()", 0);
        window.onunload = function () { null };
    </script>
</head>
<body style="overflow:hidden;">
    @if (TempData["AlertMessage"] != null)
    {
        <script type="text/javascript">
            alert("@TempData["AlertMessage"]");
        </script>
        TempData["AlertMessage"] = null;
    }


 
       
