<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <title>IFSC</title>
    <!--favicon-->
   
    <link rel="stylesheet" href="~/Content/assets/css/backend-plugin.min28b5.css?v=2.0.0">
    <link rel="stylesheet" href="~/Content/assets/css/backend28b5.css?v=2.0.0">
    <link rel="stylesheet" href="~/Content/assets/vendor/%40fortawesome/fontawesome-free/css/all.min.css">
    <link rel="stylesheet" href="~/Content/assets/vendor/line-awesome/dist/line-awesome/css/line-awesome.min.css">
    <link rel="stylesheet" href="~/Content/assets/vendor/remixicon/fonts/remixicon.css">
    <link rel="stylesheet" href="~/Content/assets/vendor/%40icon/dripicons/dripicons.css">

    <link rel='stylesheet' href='~/Content/assets/vendor/fullcalendar/core/main.css' />
    <link rel='stylesheet' href='~/Content/assets/vendor/fullcalendar/daygrid/main.css' />
    <link rel='stylesheet' href='~/Content/assets/vendor/fullcalendar/timegrid/main.css' />
    <link rel='stylesheet' href='~/Content/assets/vendor/fullcalendar/list/main.css' />
    <link rel="stylesheet" href="~/Content/assets/vendor/mapbox/mapbox-gl.css"> 
</head>
<body>
    <div class="wrapper">
        <section class="login-content">
            <div class="container h-100">
                <div class="row align-items-center justify-content-center h-100">
                    <div class="col-12">
                        <div class="row align-items-center">
                            @if (TempData["AlertMessage"] != null)
                            {
                                <script type="text/javascript">
                                    alert("@TempData["AlertMessage"]");
                                </script>
                                TempData["AlertMessage"] = null;
                            }




                            @RenderBody()

                            <div class="col-lg-6 mb-lg-0 mb-4 mt-lg-0 mt-4">
                                <img src="~/Content/assets/images/login/01.png" class="img-fluid w-80" alt="">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>

    <div class="custom-control-inline change-rtl">

        <a href="#" class="switch-rtl" data-active="true" for="rtl-mode" data-mode="rtl">RTL</a>
    </div>





    <!-- Backend Bundle JavaScript -->
    <script src="~/Content/assets/js/backend-bundle.min.js"></script>

    <!-- Flextree Javascript-->
    <script src="~/Content/assets/js/flex-tree.min.js"></script>
    <script src="~/Content/assets/js/tree.js"></script>

    <!-- Table Treeview JavaScript -->
    <script src="~/Content/assets/js/table-treeview.js"></script>

    <!-- Masonary Gallery Javascript -->
    <script src="~/Content/assets/js/masonry.pkgd.min.js"></script>
    <script src="~/Content/assets/js/imagesloaded.pkgd.min.js"></script>

    <!-- Mapbox Javascript -->
    <script src="~/Content/assets/js/mapbox-gl.js"></script>
    <script src="~/Content/assets/js/mapbox.js"></script>

    <!-- Fullcalender Javascript -->
    <script src='~/Content/assets/vendor/fullcalendar/core/main.js'></script>
    <script src='~/Content/assets/vendor/fullcalendar/daygrid/main.js'></script>
    <script src='~/Content/assets/vendor/fullcalendar/timegrid/main.js'></script>
    <script src='~/Content/assets/vendor/fullcalendar/list/main.js'></script>

    <!-- SweetAlert JavaScript -->
    <script src="~/Content/assets/js/sweetalert.js"></script>

    <!-- Vectoe Map JavaScript -->
    <script src="~/Content/assets/js/vector-map-custom.js"></script>

    <!-- Chart Custom JavaScript -->
    <script src="~/Content/assets/js/customizer.js"></script>
    <script src="~/Content/assets/js/rtl.js"></script>

    <!-- Chart Custom JavaScript -->
    <script src="~/Content/assets/js/chart-custom.js"></script>

    <!-- slider JavaScript -->
